script/source = "extends Node2D

#variables
var materialSpawn;
var getReverb = AudioServer.get_bus_effect(1,0);
var fKeyPress = 0;

#for Arrays
#Checkpoint Animation
onready var CheckpointAnimPlayDefault1 = get_node(\"Checkpoints/CheckpointAnim1\");
onready var CheckpointAnimPlayDefault2 = get_node(\"Checkpoints/CheckpointAnim2\");
onready var CheckpointAnimPlayDefault3 = get_node(\"Checkpoints/CheckpointAnim3\");
onready var CheckpointAnimPlayDefault4 = get_node(\"Checkpoints/CheckpointAnim4\");
onready var CheckpointAnimPlayDefault5 = get_node(\"Checkpoints/CheckpointAnim5\");
onready var CheckpointAnimPlayDefault6 = get_node(\"Checkpoints/CheckpointAnim6\");
onready var CheckpointAnimPlayDefault7 = get_node(\"Checkpoints/CheckpointAnim7\");
onready var CheckpointAnimPlayDefault8 = get_node(\"Checkpoints/CheckpointAnim8\");

#checkpoints
onready var Checkpoint1 = get_node(\"Checkpoints/checkpoint1\");
onready var Checkpoint2 = get_node(\"Checkpoints/checkpoint2\");
onready var Checkpoint3 = get_node(\"Checkpoints/checkpoint3\");
onready var Checkpoint4 = get_node(\"Checkpoints/checkpoint4\");
onready var Checkpoint5 = get_node(\"Checkpoints/checkpoint5\");
onready var Checkpoint6 = get_node(\"Checkpoints/checkpoint6\");
onready var Checkpoint7 = get_node(\"Checkpoints/checkpoint7\");
onready var Checkpoint8 = get_node(\"Checkpoints/checkpoint8\");


