var preloadbuildModeSound = preload(\"res://Scenes/EnableDisableBuildMode.tscn\")
var preloadPlayer = preload(\"res://Scenes/characterNew.tscn\");
var preloadGatherSound = preload(\"res://Scenes/GlobalGather.tscn\");

#booleans
var hasStartedBallTimer = false;

# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"


# Called when the node enters the scene tree for the first time.
func _ready():
	#Animation Array
	var animArray = [CheckpointAnimPlayDefault1, CheckpointAnimPlayDefault2, CheckpointAnimPlayDefault3, CheckpointAnimPlayDefault4, CheckpointAnimPlayDefault5, CheckpointAnimPlayDefault6, CheckpointAnimPlayDefault7, CheckpointAnimPlayDefault8];
	#Checkpoint Array
	var checkPointArray = [Checkpoint1, Checkpoint2, Checkpoint3, Checkpoint4, Checkpoint5, Checkpoint6, Checkpoint7, Checkpoint8];
	Global.chapterNumber = 1;
	Global.hashand1Startedmoving = false;
	
	# Vary the Spawning Point based on the index of the checkpooint
	match (Global.checkpointIndex):
		0:
			for i in 8:
				var getAnim = animArray[i];
				getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(-192, 1543);
			add_child(addPlayer);
		1:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault1:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(745, 1031);
			add_child(addPlayer);
		2:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault2:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(2922, 1279);
			add_child(addPlayer);
			Checkpoint1.queue_free();
		3:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault3:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(4702, 340);
			add_child(addPlayer);
			for j in 2:
				var CheckpointDelete = checkPointArray[j];
				CheckpointDelete.queue_free();
		4:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault4:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(4420, -734);
			add_child(addPlayer);
			for j in 3:
				var CheckpointDelete = checkPointArray[j];
				CheckpointDelete.queue_free();
		5:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault5:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(-528, -338);
			add_child(addPlayer);
			for j in 4:
				var CheckpointDelete = checkPointArray[j];
				CheckpointDelete.queue_free();
		6:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault6:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(5998, -1434);
			add_child(addPlayer);
			$Enemies/enemy4.queue_free();
			for j in 5:
				var CheckpointDelete = checkPointArray[j];
				CheckpointDelete.queue_free();
		7:
			for i in 8:
				var getAnim = animArray[i];
				if getAnim == CheckpointAnimPlayDefault8:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
