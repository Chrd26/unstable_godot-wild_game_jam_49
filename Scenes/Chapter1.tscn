				if getAnim == CheckpointAnimPlayDefault8:
					getAnim.play(\"Activated\");
				else:
					getAnim.play(\"Default-Deactivated\");
			var addPlayer = preloadPlayer.instance();
			addPlayer.position = Vector2(12062, 1295);
			add_child(addPlayer);
			$Enemies/enemy4.queue_free();
			for j in 7:
				var CheckpointDelete = checkPointArray[j];
				CheckpointDelete.queue_free();

func _input(event):
	if event.is_action_pressed(\"stacking_mode_enable\") && fKeyPress == 0:
		Global.movementEnabled = false;
		Global.hasActivatedBuilding = true;
		fKeyPress += 1;
		var playSoundBuild = preloadbuildModeSound.instance();
		add_child(playSoundBuild);
	elif event.is_action_pressed(\"stacking_mode_enable\") && fKeyPress == 1:
		Global.movementEnabled = true;
		Global.hasActivatedBuilding = false;
		fKeyPress = 0;
		var playSoundBuild = preloadbuildModeSound.instance();
		add_child(playSoundBuild);
	elif event.is_action_pressed(\"Reset\"):
		Global.lives = 3;
		Global.materials = 10;
		queue_free(); 
		# warning-ignore:return_value_discarded
		get_tree().change_scene(\"res://Scenes/Chapter1.tscn\");

func _process(_delta):
	#Play Bombs Sequence
	if Global.hashand1Startedmoving:
		if !hasStartedBallTimer:
			hasStartedBallTimer = true;
			$cannonBallTimer.start();
	if !Global.hashand1Startedmoving && hasStartedBallTimer:
		hasStartedBallTimer = false;
		$cannonBallTimer.stop();
	#Picking a Shape
	if Global.haspickedUpShape:
		Global.materials += 5;
		var playSoundGather = preloadGatherSound.instance();
		add_child(playSoundGather);
		Global.haspickedUpShape = false;
	#Death and Load Scene
	if Global.lives <= 0:
		Global.lives = 3;
		Global.materials = 10;
		queue_free(); 
		# warning-ignore:return_value_discarded
		get_tree().change_scene(\"res://Scenes/Chapter1.tscn\");
	#finish chapter
	if Global.isChapter1Finished:
		Global.hasEnteredMenu = true;
		Global.chapterNumber = 0;
		Global.lives = 3;
		Global.materials = 10;
		Global.checkpointIndex = 0;
		# warning-ignore:return_value_discarded
		get_tree().change_scene(\"res://Scenes/startGame.tscn\");
		Global.isChapter1Finished = false;
		queue_free();

#Checkpoint System

func _on_out_of_bounds_body_entered(body):
	if body.is_in_group(\"player\"):
		Global.lives = 0;

